{
    "name": "vitexsoftware/ease-framework",
    "description": "An PHP Framework for Ease of writing Applications (debian version)",
    "version": "1.1-stable",
    "authors": [
        {
            "name": "Vítězslav Dvořák",
            "email": "info@vitexsoftware.cz",
            "homepage": "http://v.s.cz/"
        }
    ],
    "autoload": {
        "psr-0": {
            "Mail": "/usr/share/php/Mail/"
        },
        "psr-4": {
